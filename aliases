alias ll='ls -lah'

# git
alias gst='git status --short --branch'
alias gap='git add --patch'
alias gau='git add --update'
alias gdf='git diff'
alias gds='git diff --staged'
alias gci='git commit'
alias gco='git checkout'
alias gbr='git branch'
alias gpl='git pull'
alias gph='git push'

# docker-machine
alias dm='docker-machine'
